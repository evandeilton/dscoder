# Carregar pacotes
library(tidyverse)
library(palmerpenguins)
# Análise descritiva
# Resumo estatístico
summary_stats <- penguins %>%
group_by(species) %>%
summarise(
count = n(),
mean_bill_length = mean(bill_length_mm, na.rm = TRUE),
mean_bill_depth = mean(bill_depth_mm, na.rm = TRUE),
mean_flipper_length = mean(flipper_length_mm, na.rm = TRUE),
mean_body_mass = mean(body_mass_g, na.rm = TRUE)
)
# Exibir resumo estatístico
print(summary_stats)
# Gráfico de dispersão: Comprimento do bico vs Profundidade do bico
ggplot(penguins, aes(x = bill_length_mm, y = bill_depth_mm, color = species)) +
geom_point() +
labs(title = "Comprimento do Bico vs Profundidade do Bico",
x = "Comprimento do Bico (mm)",
y = "Profundidade do Bico (mm)") +
theme_minimal()
# Gráfico de boxplot: Massa corporal por espécie
ggplot(penguins, aes(x = species, y = body_mass_g, fill = species)) +
geom_boxplot() +
labs(title = "Boxplot da Massa Corporal por Espécie",
x = "Espécie",
y = "Massa Corporal (g)") +
theme_minimal()
# Gráfico de densidade: Comprimento do bico por espécie
ggplot(penguins, aes(x = bill_length_mm, fill = species)) +
geom_density(alpha = 0.5) +
labs(title = "Densidade do Comprimento do Bico por Espécie",
x = "Comprimento do Bico (mm)",
y = "Densidade") +
theme_minimal()
